# AgentMesh Environment Variables
# Copy this file to .env and update with your values
# DO NOT commit .env to version control

# PostgreSQL Configuration
POSTGRES_DB=agentmesh
POSTGRES_USER=agentmesh
POSTGRES_PASSWORD=CHANGE_ME_IN_PRODUCTION

# Redis Configuration (if auth enabled)
REDIS_PASSWORD=CHANGE_ME_IN_PRODUCTION

# Grafana Configuration
GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=CHANGE_ME_IN_PRODUCTION

# AgentMesh Configuration
AGENTMESH_ENV=development
AGENTMESH_LOG_LEVEL=DEBUG
AGENTMESH_STORAGE_BACKEND=redis

# Observability
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4317
OTEL_SERVICE_NAME=agentmesh

# Server Configuration
AGENTMESH_WORKERS=4
